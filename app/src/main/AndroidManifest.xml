        <activity android:name=".ChordAm7_Activity" />
        <activity android:name=".ChordB_Activity" />
        <activity android:name=".ChordB7_Activity" />
        <activity android:name=".ChordBm_Activity" />
        <activity android:name=".ChordBm7_Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FileSelectorActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_file_selector" />
        <activity
            android:name=".TrainingActivity"
            android:label="@string/title_activity_training" />
        <activity
            android:name=".TuningActivity"
            android:label="@string/title_activity_Tuning" />
        <activity
            android:name=".PlayingActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_playing" />
