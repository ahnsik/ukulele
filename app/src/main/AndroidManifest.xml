        <activity android:name=".ChordCm_Activity" />
        <activity android:name=".ChordCm7_Activity" />
        <activity android:name=".ChordD_Activity" />
        <activity android:name=".ChordD7_Activity" />
        <activity android:name=".ChordDm_Activity" />
        <activity android:name=".ChordDm7_Activity" />
        <activity android:name=".ChordE_Activity" />
        <activity android:name=".ChordE7_Activity" />
        <activity android:name=".ChordEm_Activity" />
        <activity android:name=".ChordEm7_Activity" />
        <activity android:name=".ChordF_Activity" />
        <activity android:name=".ChordF7_Activity" />
        <activity android:name=".ChordFm_Activity" />
        <activity android:name=".ChordFm7_Activity" />
        <activity android:name=".ChordG_Activity" />
        <activity android:name=".ChordG7_Activity" />
